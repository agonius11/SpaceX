  "dependencies": {
    "apollo-server": "^2.12.0",
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "nodemon": "^1.19.4"
  }
}
